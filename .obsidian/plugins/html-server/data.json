{
  "port": 8080,
  "hostname": "0.0.0.0",
  "defaultFile": "Welcome.md",
  "startOnLoad": true,
  "useRibbonButons": true,
  "indexHtml": "<html>\n  <head>\n    <meta charset=\"utf-8\" />\n    <meta\n      name=\"viewport\"\n      content=\"width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, viewport-fit=cover\"\n    />\n    <title>#VAR{HTML_TITLE}</title>\n    <link rel=\"shortcut icon\" href=\"#VAR{FAVICON_URL}\" />\n    <link href=\"#VAR{CSS_FILE_URL}\" type=\"text/css\" rel=\"stylesheet\" />\n    <base href=\"/\" />\n\n  </head>\n  <body\n    class=\"#VAR{THEME_MODE} mod-windows is-frameless is-maximized is-hidden-frameless obsidian-app show-inline-title show-view-header\"\n    style=\"--zoom-factor: 1; --font-text-size: 16px\"\n  >\n    <div class=\"main-layout\">\n      <div class=\"menu\">\n        #VAR{MENU_RENDERED_CONTENT}\n      </div>\n      <div class=\"content\">\n        <div class=\"mod-header\">\n          <div\n            class=\"inline-title\"\n            contenteditable=\"true\"\n            spellcheck=\"false\"\n            tabindex=\"-1\"\n            enterkeyhint=\"done\"\n          >\n            #VAR{RENDERED_CONTENT_FILE_NAME}\n          </div>\n          #VAR{RENDERED_CONTENT}\n        </div>\n      </div>\n    </div>\n<script>\ndocument.addEventListener('DOMContentLoaded', function () {\n  function updateFolderVisibility() {\n    document.querySelectorAll('div.level_1, div.level_2, div.level_3, div.level_0,.folder').forEach(div => {\n      div.style.display = 'none';\n\n    });\n    document.querySelectorAll('.folder.selected').forEach(selectedHeading => {\n      let folder = selectedHeading.id;\n      document.querySelectorAll('div[folder=\"' + folder + '\"],.folder[folder=\"' + folder + '\"]').forEach(div => {\n        div.style.display = '';\n      });\n    });\ndocument.querySelectorAll('.level_0').forEach(div => {\n        div.style.display = '';\n      });\n    \n  }\n\n  document.querySelectorAll('.folder').forEach(heading => {\n    heading.addEventListener('click', function () {\n      this.classList.toggle('selected');\n      updateFolderVisibility();\n    });\n  });\n\n  // Initial state\n  updateFolderVisibility();\n});\n</script>\n  </body>\n</html>",
  "htmlReplaceableVariables": [
    {
      "varName": "HTML_TITLE",
      "varValue": "Notes"
    },
    {
      "varName": "FAVICON_URL",
      "varValue": "//obsidian.md/favicon.ico"
    },
    {
      "varName": "CSS_FILE_URL",
      "varValue": "/.obsidian/plugins/obsidian-http-server/app.css"
    }
  ],
  "showAdvancedOptions": true,
  "useSimpleAuth": false,
  "simpleAuthUsername": "otto",
  "simpleAuthPassword": "Blibmf35@"
}